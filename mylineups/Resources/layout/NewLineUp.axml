<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:minWidth="25px"
    android:minHeight="25px">
    <TableLayout
        android:id="@+id/radioLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0.0dp">
        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="250px"
            android:layout_height="10px">
            <EditText
                android:id="@+id/TeamNameBox"
                android:textColor="@android:color/background_dark"
                android:layout_width="180dp"
                android:layout_height="36.1dp"
                android:textSize="15dp" />
            <Button
                android:id="@+id/SAVE"
                android:width="250px"
                android:height="10px"
                android:text="Save All"
                android:layout_marginRight="0.0dp" />
        </TableRow>
    </TableLayout>
    <RadioGroup
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginBottom="0.0dp">
        <TableLayout
            android:id="@+id/radioLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="0.0dp">
            <TableRow
                android:id="@+id/tableRow1"
                android:layout_width="250px"
                android:layout_height="10px">
                <RadioButton
                    android:id="@+id/four_four_two"
                    android:width="250px"
                    android:height="10px"
                    android:text="4-4-2" />
                <RadioButton
                    android:id="@+id/four_two_three_one"
                    android:width="250px"
                    android:height="10px"
                    android:text="4-2-3-1"
                    android:layout_marginRight="0.0dp" />
            </TableRow>
            <TableRow
                android:id="@+id/tableRow2"
                android:layout_width="250px"
                android:layout_height="10px">
                <RadioButton
                    android:id="@+id/five_three_two"
                    android:width="250px"
                    android:height="10px"
                    android:text="5-3-1" />
                <RadioButton
                    android:id="@+id/four_three_three"
                    android:width="250px"
                    android:height="10px"
                    android:text="4-3-3"
                    android:layout_marginRight="0.0dp" />
            </TableRow>
        </TableLayout>
    </RadioGroup>
    <TableLayout
        android:id="@+id/PositionNamesTable"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TableRow
            android:id="@+id/tableRow4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/position1"
                android:text="" />
            <EditText
                android:id="@+id/Player1"
                android:textColor="@android:color/background_dark"
                android:layout_width="200px"
                android:layout_height="35dp"
                android:textSize="15dp" />
        </TableRow>
        <TableRow
            android:id="@+id/tableRow5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/position2"
                android:text="" />
            <EditText
                android:id="@+id/Player2"
                android:textColor="@android:color/background_dark"
                android:layout_width="200px"
                android:layout_height="35dp"
                android:textSize="15dp" />
        </TableRow>
        <TableRow
            android:id="@+id/tableRow6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/position3"
                android:text="" />
            <EditText
                android:id="@+id/Player3"
                android:textColor="@android:color/background_dark"
                android:layout_width="200px"
                android:layout_height="35dp"
                android:textSize="15dp"
                android:layout_column="1" />
        </TableRow>
        <TableRow
            android:id="@+id/tableRow7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/position4"
